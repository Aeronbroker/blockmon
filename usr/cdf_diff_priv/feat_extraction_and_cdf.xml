<composition id="cdf_diff_priv" app_id="boh">
	<install>

		<block id="feat_extraction" type="feat_extraction"  sched_type="active" threadpool="sniffer_thread" > 
			<params>
			</params>
		</block>

		<block id="CDF" type="CDF"  sched_type="active" threadpool="sniffer_thread" > 
			<params>
                                <build_cdf interval="2000"/> #millisecond
                                <cdf_param min="0" max="1500" bin="150"/> 
                                <diff_priv epsilon="0.8"/> #if -1 do not add noise
			</params>
		</block>

		<blockref local_block_id="sniffer" ext_composition_id="arch_input" ext_block_id="sniffer"/>

		<connection src_block="sniffer" src_gate="sniffer_out" dst_block="feat_extraction" dst_gate="in_pkt"/>
		<connection src_block="feat_extraction" src_gate="out_feat" dst_block="CDF" dst_gate="in_feat"/>
	</install>

</composition>


				
